<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dinoxindustrial.app.tab_example.ParametrosFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hacienda:"
                    android:id="@+id/textView26"
                    android:layout_column="0"
                    android:textSize="@dimen/txt"
                    android:textStyle="bold"
                    android:textColor="#ff000000" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtParametrosHacienda"
                    android:textSize="@dimen/txt"
                    android:layout_column="1" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Lote:"
                    android:id="@+id/textView3"
                    android:layout_column="0"
                    android:textSize="@dimen/txt"
                    android:textStyle="bold"
                    android:textColor="#ff000000" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt"
                    android:id="@+id/txtParametrosLote"
                    android:layout_column="1" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Contratista:"
                    android:id="@+id/textView25"
                    android:layout_column="0"
                    android:textSize="@dimen/txt"
                    android:textStyle="bold"
                    android:textColor="#ff000000" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtParametrosContratista"
                    android:textSize="@dimen/txt"
                    android:layout_column="1"
                    android:minWidth="200dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Operador:"
                    android:id="@+id/textView4"
                    android:layout_column="0"
                    android:textSize="@dimen/txt"
                    android:textStyle="bold"
                    android:textColor="#ff000000" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtParametrosOperador"
                    android:textSize="@dimen/txt"
                    android:layout_column="1"
                    android:minWidth="200dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>
        </TableLayout>
    </RelativeLayout>
</FrameLayout>
